{
  "name": "atcoder-python",
  "dockerComposeFile": "docker-compose.yml",
  "service": "python",
  "workspaceFolder": "/atcoder_python",
  "remoteEnv": {
    "../../atcoder_python": "/atcoder_python"
  },
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash",
        "github.copilot.enable": {
          "*": false
        }
      },
      "extensions": [
        "ms-python.python",
        "ms-python.flake8",
        "editorconfig.editorconfig"
      ]
    }
  },
  "remoteUser": "vscode"
}